<%= simple_form_for(@work_order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :work_order_uid %>
    <%= f.input :work_order_number %>
    <%= f.input :completed %>
    <%= f.input :slot_start %>
    <%= f.input :slot_end %>
    <%= f.input :priority %>
    <%= f.association :technician %>
    <%= f.input :tech_login_id %>
    <%= f.input :previous_fsr_login_id %>
    <%= f.input :job_id %>
    <%= f.input :dwelling_type %>
    <%= f.input :job_type %>
    <%= f.input :job_class %>
    <%= f.input :requirements_list %>
    <%= f.input :privilege_list %>
    <%= f.input :routing_area_code %>
    <%= f.input :routing_area %>
    <%= f.input :account_number %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :home_phone %>
    <%= f.input :call_ahead_number %>
    <%= f.input :customer_id %>
    <%= f.input :cod_amount %>
    <%= f.input :drop_type %>
    <%= f.input :processing_code %>
    <%= f.input :service_code %>
    <%= f.input :equipment_type %>
    <%= f.input :equipment_serial_number %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
